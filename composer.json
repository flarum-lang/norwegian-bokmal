{
	"name": "flarum-lang/norwegian-bokmal",
	"type": "flarum-extension",
	"description": "Norwegian Bokmål language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/norwegian-bokmal",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"nb-locale",
		"norwegian-bokmal-locale",
		"norwegian-bokmal"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/norwegian-bokmal/issues",
		"source": "https://github.com/flarum-lang/norwegian-bokmal"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
